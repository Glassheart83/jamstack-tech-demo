{
  "name": "jamstack-tech-demo",
  "version": "0.0.1",
  "description": "Tech demo using Jekyll and Webpack to build a fully featured website",
  "scripts": {
    "clean": "shx rm -rf ./_site ./src/assets",
    "start": "yarn clean && yarn webpack:dev && bundle exec jekyll serve",
    "build": "yarn clean && yarn webpack:dev && bundle exec jekyll build",
    "webpack:dev": "webpack --mode=development --progress --profile --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Glassheart83/jamstack-tech-demo.git"
  },
  "keywords": [
    "jamstack",
    "jekyll",
    "github",
    "webpack",
    "boilerplate",
    "demo"
  ],
  "author": "Samuele Farfarini <samuele.farfarini@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Glassheart83/jamstack-tech-demo/issues"
  },
  "homepage": "https://github.com/Glassheart83/jamstack-tech-demo#readme",
  "devDependencies": {
    "css-loader": "^3.5.3",
    "favicons-webpack-plugin": "^3.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "node-sass": "^7.0.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^8.0.2",
    "shx": "^0.3.2",
